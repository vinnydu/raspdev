<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="RaspDev"
            id="raspdev.commands.category">
      </category>
      <command
            name="Emulation"
            categoryId="raspdev.commands.category"
            id="raspdev.commands.sampleCommand">
      </command>
      <command
            name="Deploy"
            categoryId="raspdev.commands.category"
            id="raspdev.commands.sampleCommand2">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="raspdev.commands.sampleCommand"
            class="raspdev.handlers.SampleHandlerEmu">
      </handler>
      <handler
            commandId="raspdev.commands.sampleCommand2"
            class="raspdev.handlers.SampleHandlerDep">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="raspdev.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="RaspDev"
               mnemonic="M"
               id="raspdev.menus.sampleMenu">
            <command
                  commandId="raspdev.commands.sampleCommand"
                  mnemonic="S"
                  id="raspdev.menus.sampleCommand">
            </command>
            <command
                  commandId="raspdev.commands.sampleCommand2"
                  mnemonic="D"
                  id="raspdev.menus.sampleCommand2">
            </command>
         </menu>
      </menuContribution>
     
   </extension>

</plugin>
